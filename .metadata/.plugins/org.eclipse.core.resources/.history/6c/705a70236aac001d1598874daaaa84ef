package com.banco.projeto.testeturing.DTO;

import com.banco.projeto.testeturing.entities.User;

public class AccountDTO {
	
	private User titular;
    private int agenciaConta;
    private int numeroConta;
    private Double saldoConta;
    


    //Construtores
    public AccountDTO(User titular, int agenciaConta, int numeroConta, Double saldoInicial) {
        this.titular = titular;
        this.agenciaConta = agenciaConta;
        this.numeroConta = numeroConta;
        this.saldoConta = saldoInicial;
        
    }

    public AccountDTO() {}

    
    
    //Getters
    public User getTitular() {
        return titular;
    }

    public int getAgenciaConta() {
        return agenciaConta;
    }

    public int getNumeroConta() {
        return numeroConta;
    }

    public Double getSaldoConta() {
        return saldoConta;
    }

}
